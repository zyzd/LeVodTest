<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mstring.andtest">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.READ_LOGS" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.RECEIVE_USER_PRESENT" />

    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name=".ApiApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.vod.VodHomeActivity" />
        <activity
            android:name=".activity.vod.VideoGetActivity"
            android:label="@string/video_get" />
        <activity
            android:name=".activity.vod.VideoListActivity"
            android:label="@string/video_list" />
        <activity
            android:name=".activity.vod.ImageGetActivity"
            android:label="@string/image_get" />
        <activity
            android:name=".activity.vod.DataVideoHourActvity"
            android:label="@string/data_video_hour" />
        <activity
            android:name=".activity.vod.DataVideoDateActivity"
            android:label="@string/data_video_date" />
        <activity
            android:name=".activity.vod.DataTotalDateActivity"
            android:label="@string/data_total_date" />
        <activity
            android:name=".activity.vod.VideoListShowActivity"
            android:label="@string/video_list" />
        <activity
            android:name=".activity.vod.PlayActivity"
            android:configChanges="keyboard|screenSize|orientation|layoutDirection"
            android:label="播放界面"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="com.letvcloud.cmf.MediaService"
            android:process=":cmf" />

        <activity
            android:name="com.lecloud.skin.activity.FeedBackActivity"
            android:configChanges="keyboard|screenSize|orientation|layoutDirection"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar" />
        <activity android:name=".activity.live.LiveHomeActivity" />
        <activity
            android:name=".activity.live.LeLiveCreateActivty"
            android:label="@string/le_live_create" />
        <activity
            android:name=".activity.live.LeLiveSearchActivty"
            android:label="@string/le_live_search" />
        <activity
            android:name=".activity.live.LeLiveModifyActivty"
            android:label="@string/le_live_modify" />
        <activity
            android:name=".activity.live.LeLiveModifyCoverImgActivty"
            android:label="@string/le_live_modifyCoverImg" />
        <activity
            android:name=".activity.live.LeLiveStopActivity"
            android:label="@string/le_live_stop" />
        <activity
            android:name=".activity.live.LeLiveGetUrlActivity"
            android:label="@string/le_live_getUrl" />
        <activity
            android:name=".activity.live.LeLiveGetPushTokenActivity"
            android:label="@string/le_live_getPushToken" />
        <activity
            android:name=".activity.live.LeLiveGetPushUrlActivity"
            android:label="@string/le_live_getPushUrl" />
        <activity
            android:name=".activity.live.LeLiveGetActivityMachineStateActivity"
            android:label="@string/le_live_getActivityMachineState" />
        <activity
            android:name=".activity.live.LeLiveStreaminfoSearchActivity"
            android:label="@string/le_live_streaminfoSearch" />
        <activity
            android:name=".activity.live.LeLiveCreateRecTaskActivity"
            android:label="@string/le_live_reateRecTaskMap" />
        <activity
            android:name=".activity.live.LeLiveRecSearchResultActivity"
            android:label="@string/le_live_rec_searchResult" />
        <activity
            android:name=".activity.live.LeLiveGetPlayInfoActivity"
            android:label="@string/le_live_getPlayInfo" />

    </application>

</manifest>